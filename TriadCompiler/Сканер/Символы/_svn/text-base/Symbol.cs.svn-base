using System;
using System.Collections.Generic;
using System.Text;

namespace TriadCompiler
    {
    /// <summary>
    /// Информация о символе.
    /// </summary>
    public class Symbol
        {
        /// <summary>
        /// Конструктор
        /// </summary>
        public Symbol()
            {
            Code = Key.EndOfFile;
            }

        /// <summary>
        /// Код символа
        /// </summary>
        /// <Name>Code</Name>
        public virtual Key Code
            {
            get
                {
                return code;
                }
            set
                {
                code = value;
                }
            }

        /// <summary>
        /// Код символа
        /// </summary>
        protected Key code;
        };
    }
