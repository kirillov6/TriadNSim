(*События и полюса*)

(*Повторяющиеся имена полюсов*)
$300,300
routine R(input P,P;output Q,P)[array [10] of real Var]
initial
endi

event;

schedule A  in 0;
(*Планирование неописанного события*)
schedule B in 0;
schedule A,B in 1.231;

(*Отмена неописанного события*)
cancel A,C;

(*Блокирование/разблокирование неописанных полюсов*)
$301
available P,V;
(*Ошибка по полюсу V подавляется*)
$301
interlock P,V,W;

ende

(*Двойное входное событие*)
$300
event;
ende

event A;
ende

(*Повторяющиеся имена событий*)
$300
event A;
ende

(*Имя события, совпадающее с именем полюса*)
$300
event P;
ende

(*Имя события, совпадающее с именем переменной*)
$300
event Var;
ende

$301,301
endrout